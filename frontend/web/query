(a:Account{user_id: 12}), (a)-[:ex_car]->(n:Car) WHERE ID(n) = 191  OPTIONAL MATCH (n)<-[r:has_image]-(i:Image)  DELETE i, r  SET n.brand='Audi', n.car_name='korch1', n.engine='engine 2', n.engine_size='size 1', n.model='model 1', n.modification='model 4', n.year='model 1', n.location='Moscow', n.used_year_from=2013, n.used_year_to=2016 CREATE UNIQUE(i0:Image{src:'/uploads/87698f5cfc9a9abee52a1d8cc2804cf8_1490189106.jpg'})-[:has_image]->(n)